<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
  "https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
  <suppress files=".*\.java"
    checks="JavadocMethodCheck"
    lines="1-50"
    message="Ignore first 50 lines for JavadocMethodCheck." />
  <suppress files=".*\.java"
    checks="JavadocMethodCheck"
    lines="100-"
    message="Ignore all lines starting from 100 for JavadocMethodCheck." />
</suppressions>
